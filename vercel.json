{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/smart-ogso-selector",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|otf|woff|woff2|ttf|eot|svg|png|jpg|jpeg|gif|ico|json))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}